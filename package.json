{
  "name": "rn-compute-bench",
  "version": "0.1.2",
  "description": "CPU performance benchmarking library for React Native with MD5 brute-force implementations in JavaScript, Rust, and Kotlin",
  "workspaces": [
    "example"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "react-native": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist",
    "android",
    "ios",
    "cpp",
    "react-native.config.js",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "lint": "biome check",
    "codegen": "craby codegen",
    "build": "craby build && tsdown",
    "rebuild": "yarn codegen && yarn build",
    "clean": "craby clean",
    "doctor": "craby doctor",
    "show": "craby show"
  },
  "keywords": [
    "react-native",
    "android",
    "ios",
    "benchmark",
    "performance",
    "cpu",
    "rust",
    "kotlin",
    "md5",
    "brute-force",
    "native-module",
    "turbomodule"
  ],
  "author": "Nerd Stack <isaacajetunmobi@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iNerdstack/rn-compute-bench.git"
  },
  "bugs": {
    "url": "https://github.com/iNerdstack/rn-compute-bench/issues"
  },
  "homepage": "https://github.com/iNerdstack/rn-compute-bench#readme",
  "dependencies": {
    "craby-modules": "^0.1.0-rc.2",
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.7",
    "@react-native-community/cli-types": "^20.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/react": "^19.0.0",
    "crabygen": "^0.1.0-rc.2",
    "react": "19.1.1",
    "react-native": "0.82.1",
    "tsdown": "^0.15.10",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "packageManager": "yarn@4.10.3"
}
